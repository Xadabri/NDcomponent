<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graf"
    app:startDestination="@id/primerFragment">

    <fragment
        android:id="@+id/primerFragment"
        android:name="com.example.ndcomponent.primerFragment"
        android:label="fragment_primer"
        tools:layout="@layout/fragment_primer" >
        <action
            android:id="@+id/action_primerFragment_to_tercerFragment2"
            app:destination="@id/tercerFragment2" />
    </fragment>
    <fragment
        android:id="@+id/cuartoFragment2"
        android:name="com.example.ndcomponent.cuartoFragment"
        android:label="fragment_cuarto"
        tools:layout="@layout/fragment_cuarto" >
        <action
            android:id="@+id/action_cuartoFragment2_to_segundoFragment2"
            app:destination="@id/segundoFragment2" />
        <action
            android:id="@+id/action_cuartoFragment2_to_tercerFragment2"
            app:destination="@id/tercerFragment2" />
    </fragment>
    <fragment
        android:id="@+id/segundoFragment2"
        android:name="com.example.ndcomponent.segundoFragment"
        android:label="fragment_segundo"
        tools:layout="@layout/fragment_segundo" >
        <action
            android:id="@+id/action_segundoFragment2_to_cuartoFragment2"
            app:destination="@id/cuartoFragment2" />
        <action
            android:id="@+id/action_segundoFragment2_to_tercerFragment2"
            app:destination="@id/tercerFragment2" />
    </fragment>
    <fragment
        android:id="@+id/tercerFragment2"
        android:name="com.example.ndcomponent.tercerFragment"
        android:label="fragment_tercer"
        tools:layout="@layout/fragment_tercer" >
        <action
            android:id="@+id/action_tercerFragment2_to_primerFragment"
            app:destination="@id/primerFragment" />
        <action
            android:id="@+id/action_tercerFragment2_to_segundoFragment2"
            app:destination="@id/segundoFragment2" />
    </fragment>
</navigation>